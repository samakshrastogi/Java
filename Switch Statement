public class format {
    
    public static void main() {
         int button=sc.nextInt();
        switch (button) {
            case 1:
                System.out.println("Hello");
                break;
            case 2:
                System.out.println("Namaste");
                break;
            case 3:
                System.out.println("Bonjour");
                break;    
            default:
                System.out.println("Invalid Choice");
                break;
        }
    }
}
