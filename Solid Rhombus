public class format {
    
    public static void main() {
        System.out.println("Solid Rhombus");
        for(int i=b;i>=0;i--){
            for(int j=1;j<=i;j++){
                System.out.print(" ");
            }
            for(int j=1;j<=5;j++){
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
